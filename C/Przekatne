#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main()
{
int tab[10][10],j,i;
srand(time(0));

for(i=0;i<10;i++)
{
        for(j=0;j<10;j++)
        {
        tab[i][j]=rand()%10;
        printf(" %d",tab[i][j]);
        }
printf("\n");
}

printf("\n");

for(i=0;i<10;i++)
{
for(j=0;j<10;j++)
{
if(i==j)
        {
        printf("%d ",tab[i][j]);
        }

}
}

printf("\n");

for(i=9;i>=0;i--)
{
for(j=9;j>=0;j--)
{
if(i==j)
        {
        printf("%d ",tab[i][j]);
        }
}
}

printf("\n");

for(i=9;i>=0;i--)
{
for(j=9;j>=0;j--)
{
if(i==j)
        {
        printf("%d ",tab[i][j]);
        }
}
}

printf("\n");
for(i=0;i<10;i++)
{
for(j=0;j<10;j++)
{
if(i+j==9)
        {
        printf("%d ",tab[i][j]);
        }
}
}

printf("\n");

for(i=9;i>=0;i--)
{
for(j=9;j>=0;j--)
{
if(i+j==9)
        {
        printf("%d ",tab[i][j]);
        }
}
}

return 0;
}

